<?xml version="1.0" encoding="UTF-8"?>
<datasources>
  <datasource
     id="ADDE.IMAGE1"
     factory="edu.wisc.ssec.mcidasv.data.TestAddeImageDataSource"
     patterns="adde://.*/imagedata(/|\?).*$"
     doesmultiples="true"
     label="Images from an ADDE server (test)"/>
  <datasource
     id="ADDE.IMAGE-2"
     factory="edu.wisc.ssec.mcidasv.data.Test2AddeImageDataSource"
     patterns="adde://.*/imagedata(/|\?).*$"
     doesmultiples="true"
     label="Images from an ADDE server (test2)"/>
  <datasource
     id="HRIT"
     factory="edu.wisc.ssec.mcidasv.data.hrit.HRITDataSource"
     fileselection="true"
     doesmultiples="true"
     label="HRIT data source"/>     
  <datasource
     id="HYDRA"
     factory="edu.wisc.ssec.mcidasv.data.hydra.MultiDimensionDataSource"
     patterns="^.*(CAL_LID_L1.*\.hdf)$"
     fileselection="true"
     doesmultiples="true"
     label="HYDRA data source"/>
  <datasource
     id="MultiSpectral"
     factory="edu.wisc.ssec.mcidasv.data.hydra.MultiSpectralDataSource"
     fileselection="true"
     doesmultiples="true"
     label="HYDRA Multi/Hyper-Spectral data source"/>
  <datasource
     id="MCIDASX"
     factory="edu.wisc.ssec.mcidasv.data.McIdasXDataSource"
     doesmultiples="true"
     label="McIDAS-X data source"/>
 
  <datasource
     id="ADDE.POINT"
     factory="edu.wisc.ssec.mcidasv.data.AddePointDataSource"
     patterns="adde://.*/pointdata/.*$,.*\.apt"
     fileselection="true"
     label="Point data from an ADDE server"/>

</datasources>
