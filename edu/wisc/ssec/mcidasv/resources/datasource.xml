<?xml version="1.0" encoding="UTF-8"?>
<datasources>
  <datasource
     id="ADDE.IMAGE"
     factory="edu.wisc.ssec.mcidasv.data.TestAddeImageDataSource"
     patterns="adde://.*/imagedata(/|\?).*$"
     doesmultiples="true"
     label="Images from an ADDE server"/>
  <datasource
     id="ADDE.IMAGE2"
     factory="edu.wisc.ssec.mcidasv.data.Test2AddeImageDataSource"
     patterns="adde://.*/imagedata(/|\?).*$"
     doesmultiples="true"
     label="Images2 from an ADDE server"/>
  <datasource
     id="HYDRA"
     factory="edu.wisc.ssec.mcidasv.data.hydra.MultiDimensionDataSource"
     fileselection="true"
     doesmultiples="true"
     label="HYDRA data source"/>
  <datasource
     id="MultiSpectral"
     factory="edu.wisc.ssec.mcidasv.data.hydra.MultiSpectralDataSource"
     fileselection="true"
     doesmultiples="true"
     label="HYDRA Multi/Hyper-Spectral data source"/>
  <datasource
     id="MCIDASX"
     factory="edu.wisc.ssec.mcidasv.data.McIdasXDataSource"
     doesmultiples="true"
     label="McIDAS-X data source"/>
</datasources>
