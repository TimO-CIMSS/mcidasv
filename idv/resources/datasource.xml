<?xml version="1.0" encoding="UTF-8"?>
<datasources>
  <datasource
     id="CACHED"
     factory="ucar.unidata.data.CacheDataSource"
     label="Cached data source"/>


  <datasource
     id="DODS.GRID"
     factory="ucar.unidata.data.grid.DodsGeoGridDataSource"
     patterns="http://.*/dods/.*$,dods://.*$"
     label="Grids from an OPeNDAP server"/>
  <datasource
     id="DODS"
     factory="ucar.unidata.data.grid.DodsGeoGridDataSource"
     patterns="http://.*/dods/.*$,dods://.*$"
     label="Grids from an OPeNDAP server"/>
  <datasource
     id="ADDE.IMAGE"
     factory="ucar.unidata.data.imagery.AddeImageDataSource"
     patterns="adde://.*/imagedata(/|\?).*$"
     doesmultiples="true"
     label="Images from an ADDE server"/>
  <datasource
     id="MCIDAS"
     factory="ucar.unidata.data.imagery.McIDASDataSource"
     doesmultiples="true"
     label="McIDAS-X data source"/>
  <datasource
     id="RAOB"
     factory="ucar.unidata.data.sounding.RaobDataSource"/>
  <datasource
     id="ADDE.POINT"
     factory="ucar.unidata.data.point.AddePointDataSource"
     patterns="adde://.*/pointdata/.*$"
     label="Point data from an ADDE server"/>
  <datasource
     id="ADDE.PROFILER"
     factory="ucar.unidata.data.profiler.AddeProfilerDataSource"
     patterns="adde://.*/pointdata/.*$"
     label="Wind Profiler Data"/>
<!--
   <datasource id="ADDE.RADAR" factory="ucar.unidata.data.imagery.AddeImageDataSource" />
 -->
  <datasource
     id="FILE.TRACK"
     factory="ucar.unidata.data.sounding.TrackDataSource"
     patterns="c130.*\.nc,p3.*.nc$,P3.*\.nc$,.*WMI_Lear.*\.nc$"
     fileselection="true"
     doesmultiples="true"
     label="Track files"/>
  <datasource
     id="FILE.SONDE"
     factory="ucar.unidata.data.sounding.SondeDataSource"
     patterns="GLASS.*\.nc$,class.*\.nc$,sgp.*cdf$,SGP.*cdf$,sgp.*nc$,.*QCcdf,.*QC\.nc$$"
     fileselection="true"
     doesmultiples="false"
     label="Upsonde/Dropsonde files"/>
  <datasource
     id="NetCDF.METAR"
     factory="ucar.unidata.data.point.NetcdfMetarDataSource"
     patterns="_metar\.nc$,.*metar.*\.nc$,.*_syn\.nc,.*_buoy\.nc$"
     fileselection="false"
     doesmultiples="true"
     label="Netcdf METAR/Synoptic files"/>
  <datasource
     id="NetCDF.POINT"
     factory="ucar.unidata.data.point.NetcdfPointDataSource"
     patterns="_metar\.nc$,.*metar.*\.nc$,.*_syn\.nc$,.*_buoy\.nc$,.*_sounder_.*\.nc$"
     doesmultiples="true"
     fileselection="true"
     label="Netcdf Point Data files"/>
  <datasource
     id="NetCDF.SWEEP"
     factory="ucar.unidata.data.radar.NetcdfRadarDataSource"
     patterns="ncswp.*\.nc$"
     fileselection="true"
     doesmultiples="true"
     label="Netcdf Radar Sweep files"/>
<!-- 
dorade begins with 'swp' and ends with an '_'.
This allows  us to not pick up the ones that end in .part, which
are the ones being written to.
 -->
  <datasource
     id="FILE.DORADERADAR"
     patterns="swp\..*_$,.*dorade.*"
     fileselection="true"
     doesmultiples="true"
     factory="ucar.unidata.data.radar.DoradeDataSource"
     label="DORADE Radar files"></datasource>
  <datasource
     id="NetCDF.Grid"
     factory="ucar.unidata.data.grid.GeoGridDataSource"
     patterns="\.nc$,\.cdf$,.*netCDF.*,\.ncml$,\.wmo$,\.WMO$,\.grb$,\.grib$"
     fileselection="true"
     label="Grid files (netCDF/GRIB/OPeNDAP)"/>
  <datasource
     id="FILE.NETCDF"
     factory="ucar.unidata.data.grid.GeoGridDataSource"
     patterns="\.nc$,\.cdf$,.*netCDF.*,\.ncml$,\.wmo$,\.WMO$,\.grb$,\.grib$"
     fileselection="true"
     label="Netcdf files"/>
  <datasource
     id="WMS"
     factory="ucar.unidata.data.gis.WmsDataSource"
     label="WMS image data">
     patterns="\.nc$,\.cdf$,.*netCDF.*,\.ncml$,\.wmo$,\.WMO$,\.grb$,\.grib$"/>
    <property
       name="default_display"
       value="wmscontrol"/>
  </datasource>
  <datasource
     id="FILE.VIS5D"
     factory="ucar.unidata.data.grid.Vis5DDataSource"
     patterns="\.v5d$,\.TOPO$,\.vis5d"
     fileselection="true"
     label="Vis5D files"/>
  <datasource
     id="FILE.TIFF"
     factory="ucar.unidata.data.gis.GeotiffDataSource"
     patterns="\.tif$,\.tiff$"
     fileselection="true"
     label="TIFF files"/>
  <datasource
     id="FILE.TEXT"
     factory="ucar.unidata.data.text.TextDataSource"
     patterns="\.htm$,\.html$,\.txt$,\.text$,\.php$,\/$"
     fileselection="true"
     label="Text or HTML files">
    <property
       name="show_in_tree"
       value="false"/>
    <property
       name="default_display"
       value="textdisplay"/>
  </datasource>

  <datasource
     id="FILE.AUDIO"
     factory="ucar.unidata.data.text.TextDataSource"
     patterns="\.au$,\.rmf$,\.mid$,\.wav$,\.aif$,\.aiff$"
     fileselection="true"
     label="Audio File">
    <property
       name="show_in_tree"
       value="false"/>
    <property
       name="default_display"
       value="audiodisplay"/>
  </datasource>


  <datasource
     id="FILE.LOCATION"
     factory="ucar.unidata.data.text.TextDataSource"
     patterns="\.wwp$"
     fileselection="true"
     label="Locations">
    <property
       name="show_in_tree"
       value="false"/>
    <property
       name="default_display"
       value="locationcontrol"/>
  </datasource>

  <datasource
     id="FILE.CHATLOG"
     factory="ucar.unidata.data.text.TextDataSource"
     patterns="\.txt$$"
     fileselection="true"
     label="Chat Log file">
    <property
       name="show_in_tree"
       value="false"/>
    <property
       name="default_display"
       value="chatlogdisplay"/>
  </datasource>

  <datasource
     id="FILE.IMAGEMOVIE"
     factory="ucar.unidata.data.text.TextDataSource"
     patterns="\.xml$$"
     fileselection="true"
     label="Image Movie Index">
    <property
       name="show_in_tree"
       value="false"/>
    <property
       name="default_display"
       value="imagemoviecontrol"/>
  </datasource>
  <datasource
     id="FILE.DRAWING"
     factory="ucar.unidata.data.text.DrawingDataSource"
     patterns="\.xgrf$"
     fileselection="true"
     label="Drawing files">
    <property
       name="show_in_tree"
       value="false"/>
    <property
       name="default_display"
       value="curvecontrol"/>
  </datasource>
  <datasource
     id="FILE.IMAGEXML"
     factory="ucar.unidata.data.imagery.ImageXmlDataSource"
     patterns="\.ximg$"
     fileselection="true"
     label="Image XML files"></datasource>
  <datasource
     id="MOVIE"
     factory="ucar.unidata.data.text.MovieDataSource"
     patterns="\.mov$"
     fileselection="true"
     label="Movies">
    <property
       name="show_in_tree"
       value="false"/>
    <property
       name="default_display"
       value="moviedisplay"/>
  </datasource>
  <datasource
     id="FILE.ADDETEXT"
     factory="ucar.unidata.data.text.AddeTextDataSource"
     patterns="^adde://.*text.*"
     fileselection="true"
     label="Adde Text">
    <property
       name="show_in_tree"
       value="false"/>
    <property
       name="default_display"
       value="textdisplay"/>
  </datasource>
  <datasource
     id="FILE.AREAFILE"
     factory="ucar.unidata.data.imagery.McIDASImageDataSource"
     patterns=".*AREA....$,.*\.gempak\.images\.sat.*,.*goes8.*,.*\.area$"
     doesmultiples="true"
     fileselection="true"
     label="McIDAS AREA files"></datasource>
  <datasource
     id="FILE.LEVEL2RADAR"
     patterns=".*6500KHGX.*$,.*NEX.*$,.*\.raw$"
     fileselection="true"
     doesmultiples="true"
     factory="ucar.unidata.data.radar.Level2RadarDataSource"
     label="Level II Radar files"></datasource>
  <datasource
     id="FILE.SHAPEFILE"
     patterns="\.shp$,\.zip$"
     fileselection="true"
     factory="ucar.unidata.data.gis.ShapeFileDataSource"
     label="Shape files">
    <property
       name="default_display"
       value="shapefilecontrol"/>
  </datasource>
  <datasource
     id="FILE.DEM"
     patterns="\.dem$,\.asc$"
     fileselection="true"
     factory="ucar.unidata.data.gis.DemDataSource"
     label="DEM files"/>
  <datasource
     id="FILE.POINTTEXT"
     factory="ucar.unidata.data.point.TextPointDataSource"
     patterns="\.csv$,\.tsv$,\.txt$"
     fileselection="true"
     label="Text Point Data files"/>
<!-- This needs to be last  -->
  <datasource
     id="FILE.ANY"
     factory="ucar.unidata.data.VisadDataSource"
     patterns=".*"
     fileselection="true"
     label="VisAD Form files"/>
</datasources>
