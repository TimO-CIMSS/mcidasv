<?xml version="1.0" encoding="UTF-8"?>
<choosers>
  <panel
     title="Test Images"
     layout="border"
     tabTooltip="Testing a new Images chooser">
    <tabbedpane
       place="Center"
       tabnested="true">
      <chooser
         id="chooser.testimages.full"
         title="Test Images"
         menutitle="Test Images"
         showinmenu="true"
         class="ucar.unidata.idv.chooser.mcidas.TestImageChooser"/>
      <chooser
         id="chooser.testimages.default.goes-east"
         title="GOES-EAST"
         menutitle="Default Sets"
         showinmenu="true"
         class="ucar.unidata.idv.chooser.mcidas.TestDefaultChooser"/>
      <chooser
         id="chooser.testimages.default.goes-west"
         title="GOES-WEST"
         menutitle="Default Sets"
         showinmenu="true"
         class="ucar.unidata.idv.chooser.mcidas.TestDefaultChooser"/>
    </tabbedpane>
  </panel>
  <panel
     title="Command Line"
     layout="border"
     tabTooltip="Command line interface to McIDAS-X">
      <chooser
         id="chooser.commandline"
         title="Command Line"
         menutitle="Command Line"
         xhowmenu="true"
         class="ucar.unidata.idv.chooser.mcidas.McCommandChooser"/>
  </panel>
